{
  "timeZone": "America/Chicago",
  "dependencies": {},
  "exceptionLogging": "STACKDRIVER",
  "runtimeVersion": "V8",
  "oauthScopes": [
    "https://www.googleapis.com/auth/script.external_request",
    "https://www.googleapis.com/auth/spreadsheets",
    "https://www.googleapis.com/auth/script.container.ui"
  ],
  "addOns": {
    "common": {
      "name": "TimeGPT Google Sheets",
      "logoUrl": "https://raw.githubusercontent.com/leonelhrp/timegpt-google-sheet-add-on/main/img/nixtla-google-sheets-logo.png",
      "universalActions": [
        {
          "label": "Open Support URL",
          "openLink": "https://dashboard.nixtla.io/terms-of-service-and-support.html"
        }
      ]
    },
    "sheets": {
      "homepageTrigger": {
        "enabled": true,
        "runFunction": "showSidebar"
      }
    }
  },
  "executionApi": {
    "access": "ANYONE"
  },
  "urlFetchWhitelist": [
    "https://dashboard.nixtla.io/"
  ]
}